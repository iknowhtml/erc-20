{
  "ecmaVersion": "7",
  "libs": [
    "chai"
  ],
  "loadEagerly": [],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "doc_comment": true,
    "complete_strings": {
      "maxLength": 15
    },
    "node_resolve": {},
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "es_modules": {},
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    }
  }
}